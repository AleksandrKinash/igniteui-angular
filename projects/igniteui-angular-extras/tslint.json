{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "igx",
      "camelCase"
    ],
    "deprecation":false,
    "component-selector": [
      true,
      "element",
      "igx",
      "kebab-case"
    ],
    "no-input-rename": [
      false
    ],
    "no-output-on-prefix": [
      false
    ]
  }
}
