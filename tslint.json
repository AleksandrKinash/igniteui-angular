{
  "extends": "tslint:latest",
  "rules": {
	  "indent": [true, "spaces"],
	  "variable-name": [true, "allow-leading-underscore"],
	  "interface-name": [true, "always-prefix"],
	  "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
      "no-empty": false,
	  "no-bitwise": false,
	  "max-classes-per-file": [false]
  }
}