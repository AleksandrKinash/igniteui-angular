{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "igniteui-dev-demos": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist/igniteui-dev-demos",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/web.config"
                        ],
                        "styles": [
                            "src/styles/igniteui-theme.scss"
                        ],
                        "scripts": [],
                        "es5BrowserSupport": true
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                "type": "anyComponentStyle",
                                "maximumWarning": "6kb"
                            }],
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        },
                        "hmr": {
                            "budgets": [{
                                "type": "anyComponentStyle",
                                "maximumWarning": "6kb"
                            }],
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.hmr.ts"
                            }]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "igniteui-dev-demos:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "igniteui-dev-demos:build:production"
                        },
                        "hmr": {
                            "hmr": true,
                            "hmrWarning": false,
                            "browserTarget": "igniteui-dev-demos:build:hmr"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "igniteui-dev-demos:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "igniteui-dev-demos-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "igniteui-dev-demos:serve"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "igniteui-angular": {
            "root": "projects/igniteui-angular",
            "sourceRoot": "projects/igniteui-angular/src",
            "projectType": "library",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/igniteui-angular/tsconfig.lib.json",
                        "project": "projects/igniteui-angular/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "project": "projects/igniteui-angular/ng-package.prod.json",
                            "tsConfig": "projects/igniteui-angular/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/igniteui-angular/src/test.ts",
                        "tsConfig": "projects/igniteui-angular/tsconfig.spec.json",
                        "karmaConfig": "projects/igniteui-angular/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/igniteui-angular/tsconfig.lib.json",
                            "projects/igniteui-angular/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "igniteui-angular-extras": {
            "projectType": "library",
            "root": "projects/igniteui-angular-extras",
            "sourceRoot": "projects/igniteui-angular-extras/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/igniteui-angular-extras/tsconfig.lib.json",
                        "project": "projects/igniteui-angular-extras/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/igniteui-angular-extras/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/igniteui-angular-extras/src/test.ts",
                        "tsConfig": "projects/igniteui-angular-extras/tsconfig.spec.json",
                        "karmaConfig": "projects/igniteui-angular-extras/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/igniteui-angular-extras/tsconfig.lib.json",
                            "projects/igniteui-angular-extras/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "igniteui-angular-e2e": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/igniteui-angular-e2e",
            "sourceRoot": "projects/igniteui-angular-e2e/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/igniteui-angular-e2e",
                        "index": "projects/igniteui-angular-e2e/src/index.html",
                        "main": "projects/igniteui-angular-e2e/src/main.ts",
                        "polyfills": "projects/igniteui-angular-e2e/src/polyfills.ts",
                        "tsConfig": "projects/igniteui-angular-e2e/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/igniteui-angular-e2e/src/favicon.ico",
                            "projects/igniteui-angular-e2e/src/assets"
                        ],
                        "styles": [
                            "projects/igniteui-angular-e2e/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/igniteui-angular-e2e/src/environments/environment.ts",
                                    "with": "projects/igniteui-angular-e2e/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "igniteui-angular-e2e:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "igniteui-angular-e2e:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "igniteui-angular-e2e:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/igniteui-angular-e2e/src/test.ts",
                        "polyfills": "projects/igniteui-angular-e2e/src/polyfills.ts",
                        "tsConfig": "projects/igniteui-angular-e2e/tsconfig.spec.json",
                        "karmaConfig": "projects/igniteui-angular-e2e/karma.conf.js",
                        "assets": [
                            "projects/igniteui-angular-e2e/src/favicon.ico",
                            "projects/igniteui-angular-e2e/src/assets"
                        ],
                        "styles": [
                            "projects/igniteui-angular-e2e/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/igniteui-angular-e2e/tsconfig.app.json",
                            "projects/igniteui-angular-e2e/tsconfig.spec.json",
                            "projects/igniteui-angular-e2e/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/igniteui-angular-e2e/e2e/protractor.conf.js",
                        "devServerTarget": "igniteui-angular-e2e:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "igniteui-angular-e2e:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "igniteui-dev-demos"
}
